<%@ Master Language="C#" MasterPageFile="NavigationColumn.master"
	AutoEventWireup="true" CodeFile="3 columns.master.cs" Inherits="_3_columns" %>

<asp:Content ID="cHead" ContentPlaceHolderID="phHead" runat="Server">
</asp:Content>
<asp:Content ID="cContentColumns" ContentPlaceHolderID="phContentColumns" runat="Server">
	<div id="contentcolumn" class="sixcol">
		<div class="contentbox">
			<c1:Render Markup="<%# Content %>" ID="content" runat="server" />
		</div>
	</div>
	<div class="threecol last" id="asidecolumn">
		<div class="contentbox">
			<c1:Render Markup="<%# Aside %>" ID="aside" runat="server" />
		</div>
	</div>
</asp:Content>
